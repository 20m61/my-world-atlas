  "devDependencies": {
    "@babel/preset-env": "^7.22.0",
    "@babel/preset-typescript": "^7.22.0",
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.51.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
